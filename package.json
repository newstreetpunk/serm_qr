{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "APP_ENV=development vite --host --open",
    "php": "concurrently \"APP_ENV=development vite --port 5133\" \"APP_ENV=development php -S 127.0.0.1:5134 -t ./\"",
    "build": "APP_ENV=production vite build --emptyOutDir",
    "preview": "APP_ENV=production vite preview",
    "prod": "APP_ENV=production vite build --emptyOutDir && rm -rf assets && mv _site/assets assets && mv _site/.vite/manifest.json assets/manifest.json && rm -rf _site",
    "assets_v1_sync_test": "sh -c '. ./.env && rsync -av --delete --dry-run ./assets ${USER}@${SERVER}:~/www/qr/v1'",
    "assets_v1_sync": "sh -c '. ./.env && rsync -av --delete ./assets ${USER}@${SERVER}:~/www/qr/v1'",
    "assets_v2_sync_test": "sh -c '. ./.env && rsync -av --delete --dry-run ./assets ${USER}@${SERVER}:~/www/qr/v2'",
    "assets_v2_sync": "sh -c '. ./.env && rsync -av --delete ./assets ${USER}@${SERVER}:~/www/qr/v2'",
    "show-command-v1": "printf '\\033[30;43mcd qr/v1 && git pull && cd inc && git pull && cd ../../..\\033[0m\n'",
    "show-command-v2": "printf '\\033[30;43mcd qr/v2 && git pull && cd inc && git pull && cd ../../..\\033[0m\n'",
    "connect": "pnpm show-command-v1 && pnpm show-command-v2 && sh -c '. ./.env && ssh ${USER}@${SERVER} -t \"cd ~/www; bash --login\"'",
    "\n# WINDOWS SCRIPTS:": "",
    "dev_win": "cross-env APP_ENV=development vite --host --open",
    "php_win": "concurrently \"cross-env APP_ENV=development vite --port 5133\" \"cross-env APP_ENV=development php -S 127.0.0.1:5134 -t ./\"",
    "build_win": "cross-env APP_ENV=production vite build --emptyOutDir",
    "preview_win": "cross-env APP_ENV=production vite preview",
    "prod_win": "cross-env APP_ENV=production vite build --emptyOutDir && rm -rf assets && mv _site/assets assets && mv _site/.vite/manifest.json assets/manifest.json && rm -rf _site",
    "": ""
  },
  "devDependencies": {
    "alpinejs": "^3.14.9",
    "autoprefixer": "^10.4.21",
    "bootstrap": "^5.3.7",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "dropzone": "^5.9.3",
    "jquery": "^3.7.1",
    "magnific-popup": "^1.2.0",
    "merge-stream": "^2.0.0",
    "papaparse": "^5.5.3",
    "postcss": "^8",
    "qr-code-styling": "1.9.2",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.10",
    "tailwindcss": "^4.1.11",
    "vite": "^7.1.1",
    "vite-plugin-live-reload": "^3"
  },
  "packageManager": "pnpm@10.14.0"
}
